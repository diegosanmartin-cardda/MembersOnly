<h1> All posts </h1>

<% @posts.each do |post| %>
    <div>
    <% flash.each do |type, message| %>
      <div class="alert alert-<%= type %>">
        <%= message %>
        <h2> Author: <%= post.user.username %> </h2>
      </div>
    <% end %>
        <h3><%= post.title %></h3>
        <p><%= post.desc %></p>
    </div>
<% end %>